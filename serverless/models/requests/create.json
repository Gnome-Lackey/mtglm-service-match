{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "CreateMatchSchema",
  "required": ["wins", "season", "games", "winner", "losers"],
  "properties": {
    "wins": {
      "type": "number",
      "title": "MatchWins"
    },
    "games": {
      "type": "number",
      "title": "MatchGames"
    },
    "winner": {
      "type": "string",
      "title": "MatchWinner"
    },
    "losers": {
      "type": "array",
      "items": {
        "type": "string",
        "title": "MatchLosers"
      }
    },
    "season": {
      "type": "string",
      "title": "MatchSeasonId"
    }
  }
}
