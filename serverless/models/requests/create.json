{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "CreateMatchSchema",
  "required": ["records", "season"],
  "properties": {
    "records": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "player": {
            "type": "string",
            "title": "PlayerId"
          },
          "wins": {
            "type": "integer",
            "title": "PlayerWins"
          }
        }
      }
    },
    "season": {
      "type": "string",
      "title": "MatchSeasonId"
    }
  }
}
