{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "CreateMatchSchema",
  "required": ["playerA", "playerB", "season"],
  "properties": {
    "playerA": {
      "type": "object",
      "properties": {
        "player": {
          "type": "string",
          "title": "PlayerAId"
        },
        "wins": {
          "type": "integer",
          "title": "PlayerAWins"
        },
        "losses": {
          "type": "integer",
          "title": "PlayerALosses"
        }
      }
    },
    "playerB": {
      "type": "object",
      "properties": {
        "player": {
          "type": "string",
          "title": "PlayerBId"
        },
        "wins": {
          "type": "integer",
          "title": "PlayerBWins"
        },
        "losses": {
          "type": "integer",
          "title": "PlayerBLosses"
        }
      }
    },
    "season": {
      "type": "string",
      "title": "MatchSeasonId"
    }
  }
}
