{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "MatchesResponseSchema",
  "items": {
    "type": "object",
    "title": "Match",
    "properties": {
      "id": {
        "type": "string",
        "title": "MatchId"
      },
      "isSeasonPoint": {
        "type": "boolean",
        "title": "MatchIsSeasonPoint"
      },
      "games": {
        "type": "number",
        "title": "MatchGames"
      },
      "wins": {
        "type": "number",
        "title": "MatchWins"
      },
      "winners": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "MatchWinnerIds"
        }
      },
      "losers": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "MatchLoserIds"
        }
      },
      "season": {
        "type": "string",
        "title": "MatchSeasonId"
      }
    }
  }
}
