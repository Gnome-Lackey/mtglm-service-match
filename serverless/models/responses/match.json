{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "MatchResponseSchema",
  "properties": {
    "players": {
      "type": "array",
      "items": {
        "player": {
          "type": "object",
          "properties": {
            "player": {
              "type": "string",
              "title": "PlayerId"
            },
            "wins": {
              "type": "integer",
              "title": "PlayerWins"
            },
            "losses": {
              "type": "integer",
              "title": "PlayerLosses"
            }
          }
        }
      }
    },
    "Season": {
      "type": "string",
      "title": "MatchSeasonId"
    }
  }
}
